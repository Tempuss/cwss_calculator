{
  "indent_size": 4,
  "indent_char": " ",
  "beautify.language": {
    "js": {
      "type": ["javascript", "json"],
      "filename": [".jshintrc", ".jsbeautifyrc"]
    },
    "css": ["css", "scss"],
    "html": ["htm", "html"]
  },
  "key": "ctrl+b",
  "command": "HookyQR.beautify",
  "when": "editorFocus"
}